version: "3.7"

services:
  solr:
    image: gudari/solr:<%= version %>
    container_name: solr
    hostname: solr
    env_file:
      - ./solr.env

  hoster:
    image: dvdarias/docker-hoster
    container_name: hadoop_hoster
    hostname: hadoop_hoster
    volumes:
      - /var/run/docker.sock:/tmp/docker.sock
      - /etc/hosts:/tmp/hosts
